        "dazzle", "dazzle-php", "async", "non-blocking", "websocket", "socket", "server", "socket-server", "client", "socket-client"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/websocket/issues",
        "source": "https://github.com/dazzle-php/websocket"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/websocket/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7"
    },
    "require-dev": {
